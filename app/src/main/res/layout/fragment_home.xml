<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.HomeFragment">

    <Button
        android:id="@+id/button_move_recyclerview_fragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="recyclerview fragment로 이동"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="10dp"/>

    <Button
        android:id="@+id/button_move_preference_fragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="preference fragment로 이동"
        app:layout_constraintTop_toBottomOf="@id/button_move_recyclerview_fragment"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="10dp"/>

    <Button
        android:id="@+id/button_move_camera_fragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="camera fragment로 이동"
        app:layout_constraintTop_toBottomOf="@id/button_move_preference_fragment"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="10dp"/>




</androidx.constraintlayout.widget.ConstraintLayout>